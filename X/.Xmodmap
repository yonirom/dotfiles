! Swap caps lock and escape
!remove Lock = Caps_Lock
!keysym Caps_Lock = Super_L
clear lock
clear control
keycode 66 = Control_L
add control = Control_L Control_R
! Exchange left Alt and left Win
remove mod1 = Alt_L
remove mod4 = Super_L
keycode 133 = Alt_L
keycode 64 = Super_L
add mod1 = Alt_L
add mod4 = Super_L
